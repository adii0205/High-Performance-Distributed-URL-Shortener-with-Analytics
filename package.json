{
  "name": "distributed-url-shortener",
  "version": "2.0.0",
  "description": "High-performance distributed URL shortener with analytics, authentication, and advanced features",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate": "node src/db/migrations.js",
    "worker": "node src/workers/analyticsWorker.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:load": "artillery quick --count 500 -n 50 http://localhost:3000",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "url-shortener",
    "distributed",
    "analytics",
    "redis",
    "postgresql",
    "jwt",
    "graphql",
    "swagger"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.10.0",
    "redis": "^4.6.7",
    "bull": "^4.11.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "joi": "^17.10.0",
    "uuid": "^9.0.0",
    "geoip-country": "^3.0.0",
    "ua-parser-js": "^1.0.37",
    "qrcode": "^1.5.3",
    "node-cron": "^3.0.2",
    "jsonwebtoken": "^9.1.2",
    "bcryptjs": "^2.4.3",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "apollo-server-express": "^3.12.0",
    "graphql": "^16.8.0",
    "ioredis": "^5.3.2",
    "socket.io": "^4.7.2",
    "compression": "^1.7.4",
    "express-validator": "^7.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "artillery": "^2.0.3",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "@types/jest": "^29.5.5",
    "jest-config": "^29.6.4",
    "coverage": "^0.4.1"
  }
}
