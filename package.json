{
  "name": "distributed-url-shortener",
  "version": "1.0.0",
  "description": "High-performance distributed URL shortener with analytics engine",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate": "node src/db/migrations.js",
    "worker": "node src/workers/analyticsWorker.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "jest --coverage",
    "test:load": "artillery quick --count 500 -n 50 http://localhost:3000"
  },
  "keywords": [
    "url-shortener",
    "distributed",
    "analytics",
    "redis",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.10.0",
    "redis": "^4.6.7",
    "bull": "^4.11.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "joi": "^17.10.0",
    "uuid": "^9.0.0",
    "geoip-country": "^3.0.0",
    "ua-parser-js": "^1.0.37",
    "qrcode": "^1.5.3",
    "node-cron": "^3.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "artillery": "^2.0.3"
  }
}
